datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  @@map("users")
  id Int @id @default(autoincrement())
  email String? @unique
  username String @unique
  firstName String
  lastName String?
  ghToken String?
  sponsorWebhookSecret String? @unique
}
